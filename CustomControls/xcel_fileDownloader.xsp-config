<faces-config>
  <faces-config-extension>
    <namespace-uri>http://www.ibm.com/xsp/custom</namespace-uri>
    <default-prefix>xc</default-prefix>
  </faces-config-extension>
  <composite-component>
    <component-type>xcel_fileDownloader</component-type>
    <composite-name>xcel_fileDownloader</composite-name>
    <composite-file>/xcel_fileDownloader.xsp</composite-file>
    <composite-extension>
      <designer-extension>
        <in-palette>true</in-palette>
        <category>Dojo UI</category>
        <render-markup>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&#xd;
&lt;xp:view&#xd;
	xmlns:xp="http://www.ibm.com/xsp/core"&gt;&#xd;
&#xd;
	&lt;xp:div style="border:2px solid #333;"&gt;&#xd;
&#xd;
		&lt;xp:div style="background-color: #fff; padding: 4px;"&gt;&#xd;
			&lt;xp:callback&#xd;
				facetName="body"&gt;&#xd;
			&lt;/xp:callback&gt;&#xd;
		&lt;/xp:div&gt;&#xd;
	&lt;/xp:div&gt;&#xd;
&#xd;
&lt;/xp:view&gt;</render-markup>
      </designer-extension>
    </composite-extension>
    <property>
      <property-name>height</property-name>
      <property-class>string</property-class>
    </property>
    <property>
      <property-name>width</property-name>
      <property-class>string</property-class>
    </property>
    <property>
      <property-name>imagePath</property-name>
      <property-class>string</property-class>
    </property>
    <property>
      <property-name>filePath</property-name>
      <property-class>string</property-class>
    </property>
    <property>
      <property-name>fileName</property-name>
      <property-class>string</property-class>
    </property>
    <property>
      <property-name>displayName</property-name>
      <property-class>string</property-class>
    </property>
  </composite-component>
</faces-config>
