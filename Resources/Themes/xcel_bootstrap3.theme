<theme extends="bootstrapv3.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="platform:/plugin/com.ibm.designer.domino.stylekits/schema/stylekit.xsd" >
			
	<resource>
		<content-type>text/css</content-type>
		<href>/xcel_bootstrap_helper.css</href>
