	<!-- Bootstrap customizations -->
	<control>
		<name>headerNav</name>
		<property mode="override">
			<name>tagName</name>
			<value>nav</value>
		</property>
		<property>
			<name>styleClass</name>
			<value>navbar navbar-default  navbar-fixed-top</value>
		</property>	
		<property>
			<name>role</name>
			<value>navigation</value>
		</property>		
	</control>
	
	<control>
		<name>headerNavContainer</name>
		<property mode="override">
			<name>tagName</name>
			<value>div</value>
		</property>
		<property>
			<name>styleClass</name>
			<value>container-fluid</value>
		</property>		
	</control>
	
	<control>
		<name>headerNavBar</name>
		<property mode="override">
			<name>tagName</name>
			<value>div</value>
		</property>
		<property>
			<name>styleClass</name>
			<value>navbar-header</value>
		</property>		
	</control>
	
	<control>
		<name>layout.heading.title.content</name>		
		<property>
			<name>styleClass</name>
			<value>navbar-brand</value>
		</property>		
	</control>
	
	 
	<control>
		<name>navCollapseButton</name>		
		<property>
			<name>styleClass</name>
			<value>navbar-toggle</value>
		</property>
		<property>
			<name>attrs</name>
			<complex type="xp_attr">
				<property>
					<name>name</name>
					<value>data-toggle</value>
				</property>
				<property>
					<name>value</name>
					<value>collapse</value>
				</property>
			</complex>
			<complex type="xp_attr">
				<property>
					<name>name</name>
					<value>data-target</value>
				</property>
				<property>
					<name>value</name>
					<value>.navbarCollapse</value>
				</property>
			</complex>
		</property>
	</control>
	
	<control>
		<name>headerNavRight</name>
		<property mode="override">
			<name>tagName</name>
			<value>div</value>
		</property>
		<property>
			<name>styleClass</name>
			<value>collapse navbar-collapse navbarCollapse</value>
		</property>	
		<property mode="override">
			<name>id</name>
			<value>bs-example-navbar-collapse-1</value>
		</property>		
	</control>
	
	
